{
  "manifest_version": 2,

  "name": "ReveryExtension",
  "version": "0.0.0",
  "description": "Revery Chrome Extension",

"permissions": ["storage", "declarativeContent", "activeTab"],

  "background":
    {
    "scripts": ["background.js"],
    "persistent": false
    },

  "page_action": {
    "default_popup": "popup.html"
  },

  "icons": {
          "16": "16x16.png",
           "48": "48x48.png",
           "32": "32x32.png",
          "128": "128x128.png"

  }


}
